{
  "name": "phone-webapp",
  "version": "0.1.5",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start-js-secure": "HTTPS=true react-scripts start",
    "start": "cross-env NODE_PATH=./src npm-run-all -p watch-css start-js",
    "start:secure": "cross-env NODE_PATH=./src npm-run-all -p watch-css start-js-secure",
    "start:ci": "REACT_APP_ENV=ci sh -ac '. .env.ci; npm run start'",
    "build-js": "cross-env NODE_PATH=./src react-scripts build",
    "build": "cross-env npm-run-all build-css build-js",
    "test": "cross-env REACT_APP_TONE_API_PATH=external/DummyAPIClient NODE_PATH=./src react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "test:staged": "cross-env CI=true NODE_PATH=./src REACT_APP_TONE_API_PATH=external/DummyAPIClient react-scripts test --env=jsdom --findRelatedTests",
    "cypress:open": "cypress open",
    "mock-server": "node cypress/utils/mock_server.js --port 7075",
    "cypress:verify": "cypress verify",
    "cypress:version": "cypress version",
    "cypress:run": "cypress run",
    "cypress:run:ci": "cypress run --config video=false",
    "cypress:headed": "cypress run --headed",
    "cypress:canary": "cypress run --browser canary",
    "cypress:run:record": "cypress run --record"
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run test:staged --",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/third-party/**"
    ]
  },
  "dependencies": {
    "build-url": "^1.1.0",
    "debug": "^3.1.0",
    "detectrtc": "^1.3.5",
    "history": "^4.7.2",
    "i18next": "^10.6.0",
    "i18next-browser-languagedetector": "^2.1.0",
    "i18next-xhr-backend": "^1.5.1",
    "js-cookie": "^2.2.0",
    "moment": "^2.22.0",
    "prop-types": "^15.6.1",
    "qs": "^6.5.1",
    "raven-js": "^3.19.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-i18next": "^7.5.1",
    "react-notification-system": "^0.2.17",
    "react-piwik": "^1.6.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-scripts": "2.0.5",
    "redux": "^3.7.2",
    "redux-api-middleware": "^2.3.0",
    "redux-persist": "^5.9.1",
    "redux-persist-transform-filter": "0.0.16",
    "redux-thunk": "^2.2.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.83.0",
    "simple-react-timer": "^1.0.0",
    "sip.js": "^0.11.2",
    "uid": "0.0.2"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.1.5",
    "cypress": "^3.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "fetch-mock": "^6.4.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",
    "morgan": "^1.9.1",
    "node-mock-server": "^0.23.5",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.14.3",
    "react-test-renderer": "^16.3.2",
    "redux-mock-store": "^1.5.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
